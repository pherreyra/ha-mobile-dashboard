                  custom_fields:
                    message: |
                      [[[
                        const today = new Date();
                        const eventStart = new Date(entity.attributes.start_time);
                        
                        if (eventStart.toDateString() === today.toDateString()) {
                          return entity.attributes.message;
                        } else {
                          return 'No events today';
                        }
                      ]]]
                    time: |
                      [[[
                        const today = new Date();
                        const eventStart = new Date(entity.attributes.start_time);
                        const eventEnd = new Date(entity.attributes.end_time);
                        
                        if (eventStart.toDateString() === today.toDateString()) {
                          const options = { hour: '2-digit', minute: '2-digit' };
                          const startTime = eventStart.toLocaleTimeString([], options);
                          const endTime = eventEnd.toLocaleTimeString([], options);
                          return `${startTime} - ${endTime}`;
                        } else {
                          return '';
                        }
                      ]]]
                    location: |
                      [[[
                        const today = new Date();
                        const eventStart = new Date(entity.attributes.start_time);
                        
                        if (eventStart.toDateString() === today.toDateString()) {
                          let location = entity.attributes.location ? entity.attributes.location : 'No location specified';
                          if (location.length > 42) {
                            return location.substring(0, 42) + '...';  // Truncate to 42 characters and add "..."
                          }
                          return location;
                        } else {
                          return '';
                        }
                      ]]]
